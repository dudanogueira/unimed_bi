    <quoteString>N</quoteString>
    <sql>CREATE VIEW [dbo].unimed_bi_custo_assistencial 
AS

SELECT 
	DISTINCT ItemEvento.Autoid AS item_evento_autoid,
	Beneficiario.Contrato AS contrato_autoid,
	Evento.Autoid AS evento_autoid,
	evento.Beneficiario AS beneficiario_autoid,
    LEFT(lotedocservico.compapresentacao,4) AS ano_apresentacao,
    RIGHT(lotedocservico.compapresentacao,2) AS mes_apresentacao,
	lotedocservico.compapresentacao AS competencia_apresentacao,
	Evento.DataAbertura AS evento_data,
	ItemEvento.QteUtil AS quantidade,
	CAST(CONVERT(VARCHAR(10), Evento.DataAbertura, 112) AS INT) AS date_sk,
	SUM(CASE TipoValorPag
			WHEN 1
				THEN totalpago * - 1
			WHEN 2
				THEN totalpago * 1
			END) OVER (PARTITION BY ItemEvento.Autoid) AS valor_pago,
	SUM(CASE TipoValorCobr
			WHEN 1
				THEN TotalCobrado * - 1
			WHEN 2
				THEN TotalCobrado * 1
			END) OVER (PARTITION BY ItemEvento.Autoid) AS valor_cobrado,
	compvlitemevento.Prestador AS prestador_autoid,
	ServicoOperadora.Autoid AS servico_autoid,
	pAt.Autoid AS unimed_atendimento_autoid,
	pCb.Autoid AS unimed_cobranca_autoid,
	pEx.Autoid AS prestador_externo_autoid,
	CASE 
		WHEN SolicitacaoServico.Solicitante IS NULL
			THEN SolicitacaoServico.EncPrestador
		ELSE SolicitacaoServico.Solicitante
		END AS solicitante_id,
	CASE SolicitacaoServico.TipoAdmissao
		WHEN 4
			THEN 'Urgência / Emergência'
		WHEN 9
			THEN 'Eletiva / Programada / Rotina'
		ELSE 'Não Informado'
		END AS carater_atendimento,
	CONVERT(INT, ROUND(DATEDIFF(hour, pessoa.DataNascimento, Evento.DataAbertura) / 8766.0, 0)) AS idade,
	CASE 
		WHEN CONVERT(INT, ROUND(DATEDIFF(hour, pessoa.DataNascimento, Evento.DataAbertura) / 8766.0, 0)) = NULL
			THEN NULL
		WHEN CONVERT(INT, ROUND(DATEDIFF(hour, pessoa.DataNascimento, Evento.DataAbertura) / 8766.0, 0)) &lt;= 18
			THEN '0 a 18 anos'
		WHEN CONVERT(INT, ROUND(DATEDIFF(hour, pessoa.DataNascimento, Evento.DataAbertura) / 8766.0, 0)) &lt;= 23
			THEN '19 a 23 anos'
		WHEN CONVERT(INT, ROUND(DATEDIFF(hour, pessoa.DataNascimento, Evento.DataAbertura) / 8766.0, 0)) &lt;= 28
			THEN '24 a 28 anos'
		WHEN CONVERT(INT, ROUND(DATEDIFF(hour, pessoa.DataNascimento, Evento.DataAbertura) / 8766.0, 0)) &lt;= 33
			THEN '29 a 33 anos'
		WHEN CONVERT(INT, ROUND(DATEDIFF(hour, pessoa.DataNascimento, Evento.DataAbertura) / 8766.0, 0)) &lt;= 38
			THEN '34 a 38 anos'
		WHEN CONVERT(INT, ROUND(DATEDIFF(hour, pessoa.DataNascimento, Evento.DataAbertura) / 8766.0, 0)) &lt;= 43
			THEN '39 a 43 anos'
		WHEN CONVERT(INT, ROUND(DATEDIFF(hour, pessoa.DataNascimento, Evento.DataAbertura) / 8766.0, 0)) &lt;= 48
			THEN '44 a 48 anos'
		WHEN CONVERT(INT, ROUND(DATEDIFF(hour, pessoa.DataNascimento, Evento.DataAbertura) / 8766.0, 0)) &lt;= 53
			THEN '49 a 53 anos'
		WHEN CONVERT(INT, ROUND(DATEDIFF(hour, pessoa.DataNascimento, Evento.DataAbertura) / 8766.0, 0)) &lt;= 58
			THEN '54 a 58 anos'
		ELSE '59 anos ou mais'
		END AS faixa_etaria_ans,
		grupoapropriacaoutil.Codigo as grupo_apropriacao_codigo,
		ServicoOperadora.ClasseApro as classe_apropriacao_codigo
FROM compvlitemevento WITH (NOLOCK)
	INNER JOIN itemevento WITH (NOLOCK)
		ON compvlitemevento.itemevento = itemevento.autoid
	INNER JOIN evento WITH (NOLOCK)
		ON itemevento.evento = evento.autoid
	INNER JOIN lotedocservico WITH (NOLOCK)
		ON evento.lote = lotedocservico.autoid
	
	INNER JOIN beneficiario WITH (NOLOCK)
		ON evento.beneficiario = beneficiario.autoid
	INNER JOIN Pessoa WITH (NOLOCK)
		ON beneficiario.Pessoa = pEssoa.AutoId
	
	INNER JOIN ServicoOperadora WITH (NOLOCK)
		ON ItemEvento.Servico = ServicoOperadora.Autoid
	
	LEFT JOIN PrestadorServico pAt WITH (NOLOCK)
		ON evento.LcAt = pAt.AutoId
	LEFT JOIN PrestadorServico pCb WITH (NOLOCK)
		ON evento.LcCobranca = pCb.AutoId
	
	LEFT JOIN PrestadorServico pEx WITH (NOLOCK)
		ON evento.LocalExecucaoExt = pEx.AutoId
	
	LEFT JOIN SolicitacaoServico WITH (NOLOCK)
		ON ItemEvento.Autorizacao = SolicitacaoServico.AutoId
	
	LEFT JOIN grupoapropriacaoutil WITH (NOLOCK)
		ON evento.grupoapro = grupoapropriacaoutil.codigo		
WHERE 
	(itemevento.parecerauditoria = 0 OR itemevento.parecerauditoria = 2)
	--	and itemevento.AutoId = 10581619
	--  and left(lotedocservico.compapresentacao, 4) in (2017, 2018)
	--  and lotedocservico.compapresentacao = 201701
	--  and PrestadorServico.Codigo = '202310000680'
	--  and CidadePais.Codigo = 2060
	--  and PrestadorServico.Classe = 3
--	AND itemevento.autoid = 11996556</sql>
